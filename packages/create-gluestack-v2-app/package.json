{
  "name": "create-gluestack-v2-app",
  "keywords": [
    "react",
    "react-native",
    "expo",
    "storybook",
    "nextjs",
    "cross-platform",
    "cli"
  ],
  "description": "A fullstack framework.",
  "version": "0.1.30",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean-template": "rm -rf src/template/.glue/__generated__ && rm -rf src/template/package-lock.json  && rm -rf src/template/yarn.lock && rm -rf src/template/node_modules/",
    "start": "tsc --watch",
    "watch": "tsc --watch",
    "build": "yarn clean-template && tsc",
    "prepare": "yarn clean-template && tsc",
    "test": "echo 'Test passed'",
    "dev": "tsc && node dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "bin": {
    "create-gluestack-v2-app": "dist/index.js"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "gluestack",
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "@gluestack/cli-utils": "^0.1.5",
    "find-package-json": "^1.2.0",
    "fs-extra": "^11.1.1",
    "prettier": "^3.0.0",
    "simple-git": "^3.16.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/find-package-json": "^1.2.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.16",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "size-limit": "^8.2.4",
    "supertest": "^6.3.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "util": "^0.12.5"
  }
}
