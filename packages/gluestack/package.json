{
  "name": "test-size-gsui",
  "keywords": [
    "react",
    "react-native",
    "expo",
    "storybook",
    "nextjs",
    "cross-platform",
    "cli"
  ],
  "description": "A CLI tool for working with gluestack",
  "version": "1.0.13-alpha0",
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": {
    "create-gluestack": "dist/index.js"
  },
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "test": "jest",
    "test:production": "jest --isProduction=true",
    "prepare": "tsc",
    "dev": "node dist/index.js",
    "generate-template": "node scripts/generate-templates.js",
    "update-npmignore": "node scripts/update-npmignore.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "@gluestack/cli-utils": "0.1.11",
    "@types/minimist": "^1.2.2",
    "find-package-json": "^1.2.0",
    "fs-extra": "^11.1.1",
    "simple-git": "^3.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "supertest": "^6.3.3",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluestack/gluestack-ui-cli.git"
  },
  "author": "akash gautam",
  "bugs": {
    "url": "https://github.com/gluestack/gluestack-ui-cli/issues"
  },
  "homepage": "https://github.com/gluestack/gluestack-ui-cli#readme"
}
